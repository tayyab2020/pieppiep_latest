{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "anhskohbo/no-captcha": "^3.1",
        "barryvdh/laravel-dompdf": "2.2.0",
        "cartalyst/stripe-laravel": "15.0.*",
        "doctrine/inflector": "^2.0",
        "elic-dev/laravel-site-protection": "^1.0",
        "google/recaptcha": "^1.2",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.0",
        "laravel/passport": "^12.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^4.0",
        "lcobucci/jwt": "^4.3",
        "maatwebsite/excel": "^3.1",
        "mollie/laravel-mollie": "^2.0",
        "predis/predis": "^2.1",
        "spatie/laravel-permission": "^5.0",
        "symfony/http-client": "^6.4",
        "symfony/mailgun-mailer": "^6.4",
        "symfony/postmark-mailer": "^6.4",
        "symfony/symfony": "^6.4.0",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "1.0.*",
        "filp/whoops": "~2.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Markury\\": "vendor/markury/src/Adapter",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
        "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
        "@php artisan package:discover"
    ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
